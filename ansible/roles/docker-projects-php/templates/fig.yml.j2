web:
  build: .
  ports:
   - "8081:80"
  volumes:
   - {{ devhome | mandatory }}/projects:{{ devhome | mandatory }}/projects
  links: 
   - mysql
mysql:
  image: tutum/mysql:5.6
  environment:
    MYSQL_USER: "develop"
    MYSQL_PASS: "develop"
  ports:
   - "3306:3306"
  volumes:
   - {{ devhome | mandatory }}/projects/mysql/data:/var/lib/mysql